{
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "main.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/now-playing",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
